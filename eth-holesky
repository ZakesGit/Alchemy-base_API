import { JsonRpcProvider } from 'ethers';

// Connect to the Ethereum network
const provider = new JsonRpcProvider("https://eth-holesky.g.alchemy.com/v2/rS1ad-YbuD6vKG77ZwTmsaNGC19pmyyN");

// Get block by number
const blockNumber = "latest";
const block = await provider.getBlock(blockNumber);

console.log(block);
